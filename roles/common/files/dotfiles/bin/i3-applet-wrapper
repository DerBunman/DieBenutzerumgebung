#!/usr/bin/env zsh
# =====================================
# << THIS FILE IS MANAGED BY ANSIBLE >>
# =====================================
#
# DO NOT EDIT THIS FILE - CHANGES WILL BE OVERWRITTEN
# INSTEAD CHANGE THIS FILE IN THE ANSIBLE REPOSITORY.

if [[ -z "${1}" ]]; then
	echo "needs an command to wrap."
	echo "eg: ${0} indicator-cpufreq"
fi
if [ "${KILL_CMD}" ]; then
	zsh -c "${KILL_CMD}"
else
	pkill "${1:t}"
fi
#sleep 1
"$@" >/dev/null 2>&1 &!
